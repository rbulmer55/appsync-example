{
    "name": "appsync-testing",
    "version": "1.0.0",
    "description": "Velocity template appsync testing library",
    "main": "lib/index.js",
    "scripts": {
        "build": "tsc",
        "test": "jest --runInBand --testPathPattern=appsync-testing --passWithNoTests --config=../../jest.config.js",
        "test:watch": "jest --runInBand --watch --testPathPattern=appsync-testing --passWithNoTests --config=../../jest.config.js",
        "test:coverage": "jest --coverage",
        "lint": "eslint . --ext .ts",
        "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "moment-timezone": "^0.5.33",
        "velocityjs": "^2.0.3"
    },
    "devDependencies": {
        "jest": "^27.3.1"
    }
}
